/* pages/featuresProducts/featuresProductsDetail/successfulPayment/successfulPayment.wxss */
#box{
    padding: 0rpx 30rpx 0rpx 30rpx;
    box-sizing: border-box;
    margin: 0rpx;
    display: flex;
    flex-direction: column;
    align-items: center;

}
#box image{
    width: 130rpx;
    height: 130rpx;
    margin-top: 260rpx;
}
#box .success{
    font-size: 40rpx;
    font-weight: 400;
    color:rgba(51,51,51,1);
    margin-top: 33rpx;
}
.text{
    font-size: 28rpx;
    font-weight: 400;
    color:rgba(136,136,136,1);
    margin-top: 17rpx;
}
#box button{
    width: 100%;
    height: 94rpx;
    margin: 0rpx;
    background:rgba(16,142,233,1);
    border-radius:4rpx;
    color: #fff;
    margin-top: 122rpx;
}
